{
  "manifest_version": 3,
  "name": "Meow-ware",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*.jpg"],
      "matches": ["<all_urls>"]
    }
  ]
}